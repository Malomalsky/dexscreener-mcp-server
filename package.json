{
  "name": "dexscreener-mcp-server",
  "version": "1.0.0",
  "description": "Beautiful MCP server for DexScreener API with all best practices",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "dexscreener-mcp": "dist/index.js"
  },
  "scripts": {
