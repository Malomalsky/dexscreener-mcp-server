# DexScreener MCP Server Configuration

# API Configuration
DEXSCREENER_RATE_LIMIT=300  # requests per minute
DEXSCREENER_CACHE_TTL=60    # cache TTL in seconds
DEXSCREENER_TIMEOUT=30      # request timeout in seconds
DEXSCREENER_MAX_RETRIES=3   # max retry attempts

# Logging Configuration
LOG_LEVEL=INFO              # DEBUG, INFO, WARNING, ERROR
LOG_FORMAT=json             # json or text

# Development settings
DEV_MODE=false              # Enable development features